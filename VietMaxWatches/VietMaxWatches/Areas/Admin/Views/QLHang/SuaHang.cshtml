
@{
     ViewBag.Title = "SuaHang";
     Layout = "~/Areas/Admin/Views/Shared/AdminMaster.cshtml";
}

@model VietMaxWatches.Models.ThuongHieuSanPham
<h1 class="page-header">
     <a href="/Admin/QLHang/Index" class="btn btn-outline btn-primary">Quay lại</a>
</h1>
<div class="row" style="margin-bottom: 50px">

     <div class="col-md-12">
          @using (Html.BeginForm("SuaHang", "QLHang", FormMethod.Post, new { enctype = "multipart/form-data", id = "frmThem" }))
          {
               <div class="form-group input-group" style="display: block;">
                    <label>Tên Thương Hiệu</label>
                    @Html.TextBoxFor(m => m.TenThuongHieu, new { @class = "form-control", placeholder = "Tên hãng", required = "required" })
               </div>


               <div style="float: left">
                    <input type="submit" class="btn btn-outline btn-primary" value="Sửa" />
                    <input type="reset" class="btn btn-outline btn-primary" value="Làm lại" />
               </div>
          }
     </div>

</div>


