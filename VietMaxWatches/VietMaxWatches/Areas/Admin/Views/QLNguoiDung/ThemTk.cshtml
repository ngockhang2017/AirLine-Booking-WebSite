
@{
     ViewBag.Title = "ThemTk";
     Layout = "~/Areas/Admin/Views/Shared/AdminMaster.cshtml";
}


@model VietMaxWatches.Models.NguoiDung
<h1 class="page-header">
     <a href="/Admin/QLNguoiDung/Index" class="btn btn-outline btn-primary">Quay lại</a>
</h1>
<div class="row" style="margin-bottom: 50px">
     <div class="col-md-12">
          @using (Html.BeginForm("ThemTk", "QLTaiKhoan", FormMethod.Post, new { enctype = "multipart/form-data", id = "frmThem" }))
          {
               <div class="form-group input-group" style="display: block;">
                    <label>Tên tài khoản</label>
                    @Html.TextBoxFor(m => m.UserName, new { @class = "form-control", placeholder = "Tên tài khoản", required = "required" })
               </div>

               <div class="form-group input-group" style="display: block;">
                    <label>Mật khẩu</label>
                    @Html.TextBoxFor(m => m.Password, new { id = "mk", @class = "form-control", type = "password", placeholder = "Mật khẩu" })
               </div>
               <div style="float: left">
                    <input type="submit" class="btn btn-outline btn-primary" value="Thêm" />
                    <input type="reset" class="btn btn-outline btn-primary" value="Làm lại" />
               </div>
          }
     </div>

</div>

